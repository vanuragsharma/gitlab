<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\ProductExport\Block\Adminhtml\Widget\Menu" name="productexport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\ProductExport\Block\Adminhtml\Destination\Edit" name="productexport_destination_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Xtento\ProductExport\Block\Adminhtml\Destination\Edit\Tabs" name="productexport_destination_edit_tabs">
                <action method="addTab">
                    <argument name="name" xsi:type="string">general</argument>
                    <argument name="block" xsi:type="string">Xtento\ProductExport\Block\Adminhtml\Destination\Edit\Tab\Configuration</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>

