<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\ProductExport\Block\Adminhtml\Widget\Menu" name="productexport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\ProductExport\Block\Adminhtml\Tools" name="adminhtml.productexport.tools">
                <block class="Xtento\ProductExport\Block\Adminhtml\Tools\Import" name="adminhtml.productexport.tools.import" template="Xtento_ProductExport::tools/import.phtml"/>
                <block class="Xtento\ProductExport\Block\Adminhtml\Tools\Export" name="adminhtml.productexport.tools.export" template="Xtento_ProductExport::tools/export.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>

